{
    "name": "jeffochoa/twitter-cli",
    "description": "Send tweets from the console.",
    "keywords": ["framework", "laravel", "console", "zero", "twitter"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jeffochoa/twitter-cli/issues",
        "source": "https://github.com/jeffochoa/twitter-cli"
    },
    "require": {
        "php": ">=7.1",
        "laravel-zero/framework": "4.0.*",
        "toolkito/larasap": "^1.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["twitter-cli"]
}
